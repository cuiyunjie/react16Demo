{
  "name": "demo",
  "version": "1.0.0",
  "description": "this is demo",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve:dev",
    "serve:dev": "webpack-dev-server --config=webpack.config.js --content-base src/ --inline",
    "serve:static": "webpack-dev-server --config=webpack-static.config.js --content-base src/ --inline",
    "serve:dist": "webpack-dev-server --env production --config=webpack.config.js --content-base src/ --inline",
    "copy": "node -e \"require('directory-copy')({src: __dirname + '/src',dest: __dirname + '/dist',excludes: [/^\\static/]},function(err){console.log('done!')});\"",
    "clean": "rimraf src/build/* dist/*",
    "clean:src": "rimraf src/build/*",
    "release:dev": "npm run clean && webpack --config=webpack.config.js && npm run copy && npm run clean:src && node postrelease.js",
    "release:test": "npm run clean && webpack --env test --config=webpack.config.js && npm run copy && npm run clean:src && node postrelease.js",
    "release:dist": "npm run clean && webpack --env production --config=webpack.config.js && npm run copy && npm run clean:src && node postrelease.js"
  },
  "author": "sunlei",
  "license": "ISC",
  "dependencies": {
    "acorn-dynamic-import": "^3.0.0",
    "antd": "^3.5.4",
    "history": "^3.0.0",
    "prop-types": "^15.6.1",
    "rb-component": "^0.2.23",
    "rc-drawer-menu": "^1.1.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router": "^3.2.0",
    "reflux": "^6.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.8.2",
    "less": "^2.7.2",
    "less-loader": "^4.1.0",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "sass": "^1.3.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.9.7"
  }
}
