{
  "compilerOptions": {
    "outDir": "./src/build/",
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": false,
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "components/*": ["src/app/components/*"],
      "user/*": ["src/app/user/*"],
      "admin/*": ["src/app/admin/*"],
      "models/*": ["src/app/models/*"],
      "style/*": ["src/app/style/*"]
    }
  }
}